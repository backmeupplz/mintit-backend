  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "distribute": "yarn build && node --experimental-modules --es-module-specifier-resolution=node dist/app.js",
    "start": "tsc-watch --skipLibCheck --onSuccess 'node --experimental-modules --es-module-specifier-resolution=node dist/app.js'",
    "build": "tsc --skipLibCheck",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@big-whale-labs/botcaster": "^0.0.17",
    "@hapi/boom": "^10.0.0",
    "@koa/cors": "^4.0.0",
    "@standard-crypto/farcaster-js": "^5.5.2",
    "@typegoose/typegoose": "^10.0.0",
    "@types/uuid": "^9.0.0",
    "amala": "^8.0.2",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "ethers": "^5.7.2",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^12.0.0",
    "mintit-contract": "^0.0.2",
    "mongoose": "^6.8.3",
    "ultimate-text-to-image": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.8.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.1"
}
